{
  "name": "job-tracker-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "icons": "node scripts/generate-icons.mjs",
    "build": "npm run icons && vite build && shx cp manifest.json dist/manifest.json && shx cp -r icons dist/icons",
    "zip": "npm run build && cd dist && zip -r ../job-tracker-dist.zip ."
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "chrome-types": "0.1.287",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10",
    "shx": "0.3.4",
    "sharp": "0.33.4",
    "typescript": "5.5.4",
    "vite": "5.4.2"
  }
}

